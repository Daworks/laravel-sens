{
    "name": "daworks/laravel-sens",
    "description": "NCloud SENS notification channels for Lovely Laravel.",
    "keywords": [
        "laravel",
        "laravel-sms",
        "ncloud",
        "sens"
    ],
    "license": "MIT",
    "homepage": "https://daworks.io",
    "support": {
        "issues": "https://github.com/daworks/laravel-sens/issues",
        "source": "https://github.com/daworks/laravel-sens",
        "origin": "https://github.com/seungmun/laravel-sens"
    },
    "authors": [
        {
            "name": "Deokhyoung Lee",
            "email": "dhlee@daworks.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/notifications": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Daworks\\Sens\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daworks\\Sens\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daworks\\Sens\\SensServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
